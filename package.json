{
  "name": "@ape.swap/uikit",
  "version": "1.3.40-alpha.2",
  "description": "Set of UI components for apeswap projects",
  "main": "dist/index.cjs.js",
  "resolutions": {
    "**/ua-parser-js": "^0.7.30"
  },
  "module": "dist/index.esm.js",
