{
  "extends": "@apeswapfinance/eslint-config-apeswap",
  "plugins": ["@innovationupstream/styled-components"],
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": [
          "src/setupTests.[jt]s?(x)",
          "src/testHelpers.[jt]s?(x)",
          "**/*.test.[jt]s?(x)",
          "rollup.config.js"
        ]
      }
    ],
    "@innovationupstream/styled-components/no-media-query": 1,
    "@innovationupstream/styled-components/no-expression": 1
  }
}
